{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Target Injection Config",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "urls": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "files": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "regex": {
            "type": "string"
          },
          "source": {
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "output_folder": {
      "type": "string"
    }
  },
  "required": ["name", "urls", "files"],
  "additionalProperties": false
}
